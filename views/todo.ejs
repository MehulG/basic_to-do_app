<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>To-Do</title>
    <script src="/assets/jquery-3.2.1.min.js" charset="utf-8"></script>
    <script src="/assets/todo-list.js" charset="utf-8"></script>
    <link rel="stylesheet" href="/assets/styles.css">
  </head>
  <body>
    <div id="todo-table">
      <form>
        <input type="text" name="item" placeholder="enter your item here">
        <button type="submit">Add item</button>
      </form>
      <ul>
        <% for(var i = 0; i < todos.length; i++){%>
          <li><%= todos[i].item %></li>
        <%}%>
      </ul>
    </div>
  </body>
</html>
